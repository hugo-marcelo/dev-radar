    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lglugardegente.devradar"
    },
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBPbOMiuRg7iY8jhw_QWsNPlv5kd9CsGgo"
        }
      },
      "package": "com.lglugardegente.devradar",
      "adaptiveIcon": {
        "backgroundColor": "#7159c1",
        "foregroundImage": "./assets/icon.png"
      }
    },
    "packagerOpts": {
      "config": "metro.config.js"
    }
  }
}
